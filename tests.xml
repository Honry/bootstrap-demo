<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl"  href="testcase.xsl"?>
<test_definition>
  <suite name="webapi-usecase-tests">
    <set name="Multimedia &amp;amp; Graphics" background="brand-success" icon="glyphicon-facetime-video">
      <testcase component="usecase" type="functional_positive" status="approved" execution_type="manual" platform="all" priority="P0" id="Jsenhance" purpose="Jsenhance">
      </testcase>
    </set>
    <set name="Networking &amp;amp; Storage" background="brand-warning" icon="glyphicon-link">
      <testcase component="usecase" type="functional_positive" status="approved" execution_type="manual" platform="all" priority="P0" id="FileSystem" purpose="File System">
      </testcase>
      <testcase component="usecase" type="functional_positive" status="approved" execution_type="manual" platform="all" priority="P0" id="RawSockets" purpose="Raw Sockets">
      </testcase>
      <testcase component="usecase" type="functional_positive" status="approved" execution_type="manual" platform="all" priority="P0" id="WebSocket" purpose="WebSocket">
      </testcase>
      <testcase component="usecase" type="functional_positive" status="designed" execution_type="manual" platform="all" priority="P0" id="SpeechSynthesis" purpose="SpeechSynthesis">
      </testcase>
    </set>
    <set name="Security" background="brand-warning3" icon="glyphicon-warning-sign">
      <testcase component="usecase" type="functional_positive" status="approved" execution_type="manual" platform="all" priority="P0" id="CSP" purpose="CSP" subcase="9">
      </testcase>
    </set>
    <set name="Tizen Extension APIs" background="brand-primary3" icon="glyphicon-send">
      <testcase component="sample" type="functional_positive" status="approved" execution_type="manual" platform="tizen" priority="P0" id="ApplicationManager" purpose="ApplicationManager">
      </testcase>
      <testcase component="sample" type="functional_positive" status="approved" execution_type="manual" platform="tizen" priority="P0" id="AudioSystem" purpose="AudioSystem">
      </testcase>
      <testcase component="sample" type="functional_positive" status="approved" execution_type="manual" platform="tizen" priority="P0" id="Bookmark" purpose="Bookmark">
      </testcase>
      <testcase component="sample" type="functional_positive" status="approved" execution_type="manual" platform="tizen" priority="P0" id="Content" purpose="Content">
      </testcase>
      <testcase component="sample" type="functional_positive" status="approved" execution_type="manual" platform="tizen" priority="P0" id="Download" purpose="Download">
      </testcase>
      <testcase component="sample" type="functional_positive" status="approved" execution_type="manual" platform="tizen" priority="P0" id="TizenFileSystem" purpose="Tizen Filesystem">
      </testcase>
      <testcase component="sample" type="functional_positive" status="approved" execution_type="manual" platform="tizen" priority="P0" id="MessagePort" purpose="MessagePort">
      </testcase>
      <testcase component="sample" type="functional_positive" status="approved" execution_type="manual" platform="tizen" priority="P0" id="NBS" purpose="NBS">
